{
  "name": "azdo-userscripts",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "license": "MIT",
  "author": "Shane Martin",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s ci clean build:js",
    "build:js": "rollup -c",
    "ci": "run-s lint format:check",
    "clean": "del-cli dist",
    "dev": "rollup -wc",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "eslint --ext .ts,.tsx .",
    "prepare": "husky install",
    "release": "standard-version",
    "serve": "http-server -c5 dist"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": "doctoc --github --maxlevel 3",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.10",
    "@violentmonkey/dom": "^2.1.5",
    "@violentmonkey/ui": "^0.7.8",
    "solid-js": "^1.7.11"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "@gera2ld/plaid": "~2.6.0",
    "@gera2ld/plaid-rollup": "~2.6.0",
    "@unocss/postcss": "^0.55.2",
    "@violentmonkey/types": "^0.1.5",
    "babel-preset-solid": "^1.7.7",
    "del-cli": "^5.0.0",
    "doctoc": "^2.2.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "rollup-plugin-userscript": "^0.2.0",
    "sort-package-json": "^2.6.0",
    "standard-version": "^9.5.0",
    "unocss": "^0.55.2"
  },
  "engines": {
    "node": ">=18"
  }
}
